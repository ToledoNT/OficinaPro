generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Cliente {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  nome         String
  telefone     String
  whatsapp     String
  cpf          String
  endereco     String
  observacoes  String
}


model Log {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  value     String
  createdAt DateTime @default(now())
}